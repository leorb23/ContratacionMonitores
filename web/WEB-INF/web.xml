<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>  
    <servlet>
        <servlet-name>ContratacionMonitoresServlet</servlet-name>
        <servlet-class>com.umariana.contratacionmonitores.controladores.ContratacionMonitoresServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SesionServlet</servlet-name>
        <servlet-class>com.umariana.contratacionmonitores.controladores.SesionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ContratacionMonitoresServlet</servlet-name>
        <url-pattern>/ContratacionMonitoresServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SesionServlet</servlet-name>
        <url-pattern>/SesionServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
